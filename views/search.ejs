<section class="section">
  <p class="is-5">
    <strong><%= numFound %></strong> records found
  </p>
  <hr class="hr">
  <div class="container">
    <% for (let doc of docs) { %>
      <div class="box">
        <div class="media-content">
          <div class="content">
            <p>
              <%= doc.bibcode %>
              &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
              <%= doc.pubdate %>
              <br>
              <strong><%= doc.title %></strong>
              <br>
              <%= doc.author %>
            </p>
          </div>
        </div>
      </div>
    <% } %>
  </div>
</section>
